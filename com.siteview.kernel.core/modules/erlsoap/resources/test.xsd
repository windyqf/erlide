<xsd:element name="string" minOccurs="0" maxOccurs="unbounded">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:maxLength value="64"/>
<xsd:enumeration value="Subscriber"/>
</xsd:restriction>
</xsd:simpleType>
