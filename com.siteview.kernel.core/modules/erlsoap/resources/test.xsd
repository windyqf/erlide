<xsd:element name="FaultCode" type="tns:FaultCodeType"/>
<xsd:element name="FaultString" type="xsd:string" minOccurs="0"/>
<xsd:element name="SetParameterValuesFault" minOccurs="0" maxOccurs="unbounded">
