<xsd:complexType name="AllQueuedTransferStruct">
<xsd:sequence>
<xsd:element name="CommandKey" type="tns:CommandKeyType"/>
<xsd:element name="State">
<xsd:simpleType>
<xsd:restriction base="xsd:int">
<xsd:enumeration value="1">
