<xsd:documentation>CPE Vendor fault codes</xsd:documentation>
</xsd:annotation>
<xsd:minInclusive value="9800"/>
<xsd:maxInclusive value="9899"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:union>
</xsd:simpleType>
</xsd:element>
<xsd:element name="FaultString">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:maxLength value="256"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
</xsd:sequence>
</xsd:complexType>


<xsd:complexType name="DeviceIdStruct">
<xsd:sequence>
<xsd:element name="Manufacturer">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:maxLength value="64"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
<xsd:element name="OUI">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:length value="6"/>
<xsd:pattern value="[0-9A-F]{6}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
<xsd:element name="ProductClass">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:maxLength value="64"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
<xsd:element name="SerialNumber">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:maxLength value="64"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="EventStruct">
<xsd:sequence>
<xsd:element name="EventCode">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:maxLength value="64"/>
<xsd:pattern value="BOOTSTRAP"/>
<xsd:pattern value="BOOT"/>
<xsd:pattern value="PERIODIC"/>
<xsd:pattern value="SCHEDULED"/>
<xsd:pattern value="VALUE CHANGE"/>
<xsd:pattern value="KICKED"/>
<xsd:pattern value="CONNECTION REQUEST"/>
<xsd:pattern value="TRANSFER COMPLETE"/>
<xsd:pattern value="DIAGNOSTICS COMPLETE"/>
<xsd:pattern value="REQUEST DOWNLOAD"/>
<xsd:pattern value="AUTONOMOUS TRANSFER COMPLETE"/>
<xsd:pattern value="\d+( \S+)+"/>
<xsd:pattern value="M Reboot"/>
<xsd:pattern value="M ScheduleInform"/>
<xsd:pattern value="M Download"/>
<xsd:pattern value="M Upload"/>
<xsd:pattern value="M \S+"/>
<xsd:pattern value="M X_\S+"/> <!-- no spaces in method names -->
<xsd:pattern value="X [0-9A-F]{6} .*"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
<xsd:element name="CommandKey" type="tns:CommandKeyType"/>
</xsd:sequence>
</xsd:complexType>

<xsd:simpleType name="CommandKeyType">
<xsd:restriction base="xsd:string">
<xsd:maxLength value="32"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="ObjectNameType">
<xsd:restriction base="xsd:string">
<xsd:maxLength value="256"/>
<xsd:pattern value=".*\."/>
</xsd:restriction>
</xsd:simpleType>

<xsd:complexType name="EventList">
<xsd:sequence>
