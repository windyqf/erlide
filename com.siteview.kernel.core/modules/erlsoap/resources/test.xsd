</xsd:element>
<xsd:element name="IsDownload" type="xsd:boolean"/>
<xsd:element name="FileType">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:maxLength value="64"/>
<xsd:pattern value="Firmware Upgrade Image"/>
<xsd:pattern value="Web Content"/>
<xsd:pattern value="Vendor Configuration File"/>
<xsd:pattern value="Vendor Log File"/>
<xsd:pattern value="X [0-9A-F]{6} .*"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
<xsd:element name="FileSize" type="xsd:unsignedInt"/>
<xsd:element name="TargetFileName">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:maxLength value="256"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
</xsd:sequence>
</xsd:complexType>

