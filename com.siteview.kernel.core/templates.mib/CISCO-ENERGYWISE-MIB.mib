    OBJECTS         {
                        cewMaxImportance,
                        cewMaxImportanceId,
                        cewEntNeighborIndex,
                        cewEntNannyVector,
                        cewEntKeyword,
                        cewEntName,
                        cewEntRoleDescription,
                        cewEntFullName,
                        cewEntEnergyUnits,
                        cewEntEnergyUsage,
                        cewEntEnergyLevel,
                        cewEntEnergyUsageProvisioned,
                        cewEntImportanceInt,
                        cewEntImportanceExt,
                        cewEntImportanceParentId,
                        cewEntParentId,
                        cewEntImportanceRelative,
                        cewEntAdminStatus,
                        cewEntOperStatus,
                        cewLevelMaxUsage,
                        cewLevelDeltaUsage,
                        cewLevelUnits,
                        cewDeviceId,
                        cewDeviceNeighborCount,
                        cewEntEnergyUsageCaliber,
                        cewDomainName
                    }
    STATUS          current
    DESCRIPTION
        "This group contains the collection of all the objects
        related to a Cisco EnergyWise entity."
    ::= { ciscoEnergywiseMIBGroups 1 }

ciscoEnergywiseNeighborGroup OBJECT-GROUP
    OBJECTS         {
                        cewNeighborId,
                        cewNeighborType,
                        cewNeighborHeartBeat,
                        cewNeighborStatus,
                        cewNeighborStorage
                    }
    STATUS          current
    DESCRIPTION
        "This group contains the collection of all the objects
        related to a Cisco EnergyWise neighbor"
    ::= { ciscoEnergywiseMIBGroups 2 }

ciscoEnergywiseEventGroup OBJECT-GROUP
    OBJECTS         {
                        cewEventLevel,
                        cewEventRecurrence,
                        cewEventStatus,
                        cewEventTime,
                        cewEventStorage,
                        cewEventOccuredErrorCode
                    }
    STATUS          current
    DESCRIPTION
        "This group contains the collection of all the objects
        related to a Cisco EnergyWise event."
    ::= { ciscoEnergywiseMIBGroups 3 }

ciscoEnergywiseProxyGroup OBJECT-GROUP
    OBJECTS         {
                        cewProxyAddressType,
                        cewProxyAddress,
                        cewProxyPort,
                        cewProxyClass,
                        cewProxyStorage,
                        cewProxyStatus
                    }
    STATUS          current
    DESCRIPTION
        "This group contains the collection of all the objects
        related to a Cisco EnergyWise proxy."
    ::= { ciscoEnergywiseMIBGroups 4 }

